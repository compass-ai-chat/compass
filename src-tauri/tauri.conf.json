{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "compass",
  "version": "0.1.0",
  "identifier": "com.nordwestt.compass",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "npm run web",
    "beforeBuildCommand": "npm run build:web"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "Compass",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false,
        "zoomHotkeysEnabled": true,
        "center": true,
        "minWidth": 600,
        "minHeight": 400
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "externalBin": ["binaries/corsproxy"],
    "windows": {
      "wix": {
        "language": "en-US",
        "enableElevatedUpdateTask": false
      },
      "webviewInstallMode": {
        "type": "downloadBootstrapper",
        "silent": true
      },
      "digestAlgorithm": "sha256",
      "timestampUrl": "http://timestamp.digicert.com"
    },
    "publisher": "NordWestT"
  }
  
}
